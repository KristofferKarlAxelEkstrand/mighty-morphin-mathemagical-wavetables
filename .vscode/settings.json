    "axhline",
    "axvline",
    "bandlimit",
    "boxstyle",
    "capsys",
    "celerybeat",
    "certifi",
    "Chatmode",
    "chatmodes",
    "conftest",
    "coretemp",
    "crossfade",
    "dataclass",
    "Dataclasses",
    "debugpy",
    "denormal",
    "direnv",
    "dmypy",
    "docstrings",
    "dtype",
    "ehthumbs",
    "endswith",
    "envrc",
    "esbenp",
    "Exampleson",
    "facecolor",
    "figsize",
    "flac",
    "fontsize",
    "fontweight",
